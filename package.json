{
  "name": "@tiwentichat/tiwentichat",
  "version": "1.35.0",
  "description": "ChatGPT案例库。",
  "author": "tiwentichat",
  "license": "MIT",
  "homepage": "https://tiwentichat.github.io/tiwentichat",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "build": "refs-cli",
    "start": "refs-cli --watch",
    "prettier": "prettier --write '**/*.{mjs,css,json,prettierrc,lintstagedrc}'",
    "markdownlint": "npx markdownlint-cli --fix ./docs/*.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiwentichat/tiwentichat.git"
  },
  "bugs": {
    "url": "https://github.com/tiwentichat/tiwentichat/issues"
  },
  "keywords": [],
  "lint-staged": {
    "**/*.{mjs,css,json,prettierrc,lintstagedrc}": "prettier --write",
    "*.md": "markdownlint --fix",
    "docs/*.md": "markdownlint --fix"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "refs-cli": "^1.3.2",
    "markdownlint": "^0.27.0",
    "markdownlint-cli": "^0.33.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
